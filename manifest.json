{
	"name": "Laser Eyes",
	"description": "Outline every element on your website with the help of Laser Eyes",
	"version": "1.0",
	"manifest_version": 3,
	"icons": { 
		"16": "img/16.png",
		"32": "img/32.png",
		"48": "img/48.png",
		"128": "img/128.png" 
	},
	"action": {
		"default_icon": {                    
			"16": "img/16.png",
			"32": "img/32.png",
			"48": "img/48.png",
			"128": "img/128.png"       
		}
	},
	"background": {
		"service_worker": "js/background.js"
	},
	"options_page": "options.html",
	"permissions": ["storage", "activeTab", "scripting"]
}